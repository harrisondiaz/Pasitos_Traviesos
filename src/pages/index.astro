      <PrincipalCard
        tittle="En exploracion"
        subtitle="de 2 años a 5 años"
        text="Conoce nuestros productos"
        url="https://ggbralugqoodmaklkses.supabase.co/storage/v1/object/sign/public%20images/Group%2026.png?token=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJ1cmwiOiJwdWJsaWMgaW1hZ2VzL0dyb3VwIDI2LnBuZyIsImlhdCI6MTcxMDY5NjQwNCwiZXhwIjoyMzQxNDE2NDA0fQ.3VEGsKGQ8XCwb7uQmTCxZlUydJjt8ww6X1H5mXdnnEY&t=2024-03-17T17%3A26%3A46.704Z"
      />
