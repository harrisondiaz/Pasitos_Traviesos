---
import Layout from "../layouts/Layout.astro";
import Card from "../components/Card.astro";
import Mensaje from "../components/Mensaje.astro";
import Navbar from "../components/Navbar.astro";
import Promo from "../components/Promo.astro";
import ImageCard from "../components/ImageCard.astro";
import ImageCardButton from "../components/ImageCardButton.astro";
import ImageCardLeft from "../components/ImageCardLeft.astro";
import PrincipalCard from "../components/PrincipalCard.astro";
import Circle from "../components/Circle.astro";
import Footer from "../components/Footer.astro";
