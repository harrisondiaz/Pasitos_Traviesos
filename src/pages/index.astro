    >
      <PrincipalCard
        tittle="Inicio"
        subtitle="de 0 a 6 meses"
        text="Conoce nuestros productos"
        url="https://ggbralugqoodmaklkses.supabase.co/storage/v1/object/sign/public%20images/Group%2024.png?token=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJ1cmwiOiJwdWJsaWMgaW1hZ2VzL0dyb3VwIDI0LnBuZyIsImlhdCI6MTcxMDY0NDgwNSwiZXhwIjoyMzQxMzY0ODA1fQ.BQ_Nenm4CS4NcULERkcchTxiHQyle0G5HdoRIYwmKWw&t=2024-03-17T03%3A06%3A47.358Z"
      />
      <PrincipalCard
        tittle="En crecimiento"
        subtitle="de 6 meses a 2 años"
        text="Conoce nuestros productos"
        url="https://ggbralugqoodmaklkses.supabase.co/storage/v1/object/sign/public%20images/Group%2025.png?token=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJ1cmwiOiJwdWJsaWMgaW1hZ2VzL0dyb3VwIDI1LnBuZyIsImlhdCI6MTcxMDY0NzAyMywiZXhwIjoyMzQxMzY3MDIzfQ.EBlwCeZVqKPcbd6tJFxb-Lp4DD8DokVwEHm0WXFdrg0&t=2024-03-17T03%3A43%3A44.812Z"
